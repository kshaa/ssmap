    "test": "mocha",
    "format": "prettier --write \"**/*.{js,ts,json,md}\" \"!dist/**\" \"!node_modules/**\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\" \"!dist/**\" \"!node_modules/**\""
