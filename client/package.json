{
  "name": "ssmap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.22.0",
    "@babel/eslint-parser": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.3",
    "bem-cn": "^2.1.3",
    "buffer": "^6.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^4.1.2",
    "css-loader": "^6.8.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^8.43.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "leaflet": "^1.9.4",
    "mini-css-extract-plugin": "^2.7.0",
    "normalize.css": "^8.0.0",
    "object-assign": "4.1.1",
    "postcss": "^8.4.24",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^7.3.0",
    "process": "^0.11.10",
    "promise": "8.0.1",
    "querystring-es3": "^0.2.1",
    "raf": "3.4.0",
    "react": "^18.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "resolve": "1.6.0",
    "sass": "^1.32.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "url": "^0.11.0",
    "url-loader": "^4.1.1",
    "url-parse": "^1.4.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-manifest-plugin": "^5.0.0",
    "whatwg-fetch": "2.0.3",
    "workbox-webpack-plugin": "^6.6.0",
    "zod": "^4.1.13"
  },
  "scripts": {
    "dev": "node scripts/start.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"config/**/*.js\" \"scripts/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"config/**/*.js\" \"scripts/**/*.js\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs,ts,tsx}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "@typescript-eslint/semi": [
        "error",
        "never"
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      },
      {
        "files": [
          "**/*.js",
          "**/*.jsx"
        ],
        "parser": "@babel/eslint-parser",
        "parserOptions": {
          "requireConfigFile": false,
          "babelOptions": {
            "presets": [
              "@babel/preset-react"
            ]
          }
        }
      }
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.2",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/url-parse": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "prettier": "^3.7.4",
    "react-refresh": "^0.18.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0"
  }
}
